{
  "name": "@resee-movies/nuxt-ux",
  "version": "0.0.0",
  "description": "The next-gen user experience library for ReSee Movies - currently in development. ",
  "repository": {
    "url": "https://github.com/ReSee-Movies/nuxt-ux.git"
  },
  "bugs": "https://github.com/ReSee-Movies/nuxt-ux/issues",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./components/*": {
      "import": "./dist/runtime/components/*"
    },
    "./composables/*": {
      "import": "./dist/runtime/composables/*"
    },
    "./types": {
      "types": "./dist/runtime/types/index.d.ts"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.53",
    "@iconify-json/solar": "^1.2.4",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/kit": "^4.1.2",
    "@primevue/nuxt-module": "^4.4.0",
    "@resee-movies/utilities": "^0.10.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@vueuse/core": "^13.9.0",
    "primevue": "^4.4.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.2",
    "@nuxt/test-utils": "^3.19.2",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22.18.8",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/language-server": "^3.1.0",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.36.0",
    "happy-dom": "^18.0.1",
    "nuxt": "^4.1.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
