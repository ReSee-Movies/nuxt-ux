{
  "name": "@resee-movies/nuxt-ux",
  "version": "1.0.0",
  "description": "",
  "repository": "",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./components/*": {
      "import": "./dist/runtime/components/*"
    },
    "./types": {
      "types": "./dist/runtime/types/index.ts"
    },
    "./primevue": {
      "import": "./dist/runtime/theme/primevue/index.js"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.47",
    "@iconify-json/solar": "^1.2.4",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/kit": "^4.0.3",
    "@primevue/nuxt-module": "^4.3.7",
    "@tailwindcss/postcss": "^4.1.11",
    "primevue": "^4.3.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint": "^1.8.0",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "latest",
    "@vue/language-server": "^3.0.5",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "happy-dom": "^18.0.1",
    "nuxt": "^4.0.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
