{
  "name": "@resee-movies/nuxt-ux",
  "version": "0.6.0",
  "description": "The next-gen user experience library for ReSee Movies - currently in development. ",
  "repository": {
    "url": "https://github.com/ReSee-Movies/nuxt-ux.git"
  },
  "bugs": "https://github.com/ReSee-Movies/nuxt-ux/issues",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./components/*": {
      "import": "./dist/runtime/components/*"
    },
    "./composables/*": {
      "import": "./dist/runtime/composables/*.js"
    },
    "./types": {
      "types": "./dist/runtime/types/index.d.ts"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run dev:prepare && nuxi dev playground --local --port 3003",
    "dev:preview": "npm run dev:build && nuxi preview playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepare": "nuxt-module-build prepare && nuxt-module-build build",
    "release": "dotenv release-it --",
    "lint": "eslint .",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint": "^1.11.0",
    "@nuxt/eslint-config": "^1.11.0",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.20.1",
    "@release-it/conventional-changelog": "^10.0.2",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.11",
    "nuxt": "^4.2.1",
    "release-it": "^19.0.6",
    "typescript": "^5.9.3",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.5"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.61",
    "@iconify-json/solar": "^1.2.5",
    "@nuxt/fonts": "^0.12.1",
    "@primeuix/utils": "^0.6.1",
    "@primevue/forms": "^4.5.1",
    "@primevue/nuxt-module": "^4.5.1",
    "@resee-movies/utilities": "^0.10.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@vueuse/core": "^14.1.0",
    "primevue": "^4.5.1",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.13"
  }
}
